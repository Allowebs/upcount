{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "http-capability",
  "description": "HTTP capabilities for API requests",
  "windows": ["main"],
  "permissions": [
    "http:default",
    "http:allow-fetch",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://api.anthropic.com/**"
        }
      ]
    }
  ]
}